{
	"name": "unpkg-package-manager",
	"version": "0.0.7",
	"description": "offline first, fast af, package manager for the web",
	"repository": "osdevisnot/unpkg-package-manager",
	"license": "MIT",
	"author": "osdevisnot <osdevisnot@gmail.com>",
	"files": [
		"dist"
	],
	"source": "src/index.ts",
	"bin": {
		"upm": "dist/index.js"
	},
	"scripts": {
		"build": "ncc build src/index.ts -o dist",
		"postbuild": "rm -rf dist/package.json",
		"coverage": "tslib coverage",
		"coveralls": "tslib coveralls",
		"format": "tslib format",
		"lint": "tslib lint",
		"prepare": "npm run build",
		"pub": "tslib pub",
		"postpub": "docker build --no-cache -t upm .",
		"setup": "tslib setup",
		"test": "tslib test",
		"watch": "tslib watch"
	},
	"prettier": "tslib-cli",
	"dependencies": {},
	"devDependencies": {
		"@types/jest": "^24.0.18",
		"@types/node": "^12.7.11",
		"@zeit/ncc": "^0.20.5",
		"chalk": "^3.0.0-beta.1",
		"js-yaml": "^3.13.1",
		"node-fetch": "^2.6.0",
		"ora": "^4.0.2",
		"tslib-cli": "^5.0.24"
	},
	"peerDependencies": {}
}
